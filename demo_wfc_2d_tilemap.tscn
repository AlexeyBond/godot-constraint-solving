script/source = "extends Node2D


var mapper: Mapper2D = TileMapMapper.new()
var rules: WFCRules2D = WFCRules2D.new()
var problem: WFC2DProblem
var solver: WFCSolver

var mt_solver: WFCMultithreadedSolver2D

func _ready():
	mapper.learn_from($sample)

	rules.mapper = mapper
	rules.complete_matrices = true
	rules.learn_from($sample)
	rules.learn_negative_from($negative_sample)

	$sample.visible = false
	$negative_sample.visible = false
	
	var problem_settings: WFC2DProblem.WFC2DProblemSettings = WFC2DProblem.WFC2DProblemSettings.new()

	problem_settings.rect = Rect2i(0,0, 1920/16 + 1, 1080/16 + 1)
	problem_settings.rules = rules

	@warning_ignore(\"integer_division\")
	problem = WFC2DProblem.new(problem_settings, $target)
	
	#solver = WFCSolver.new(problem)
	#solver.allow_backtracking = true
	
	mt_solver = WFCMultithreadedSolver2D.new()
	mt_solver.settings.max_threads = 4
	mt_solver.settings.extra_overlap = Vector2i(1,1)
	mt_solver.solver_settings.backtracking_limit = 1000

	mt_solver.start(problem)

# var map_up_to_date = true

var spent_time: float = 0.0
var finished: bool = false

func _process(delta):
	#if solver.current_state != null:
	#	if not solver.current_state.is_all_solved():
	#		spent_time += delta
	#		for i in range(10):
	#			if not solver.current_state.is_all_solved():
	#				solver.solve_step()
	#				map_up_to_date = false
	#else:
	#	$target.clear()

	#if not map_up_to_date:
	#	map_up_to_date = true
	#	if solver.current_state.is_all_solved():
	#		print('Solved in %.1fs' % spent_time)

	#	if false or solver.current_state.is_all_solved():
	#		problem.render_state_to_map(solver.current_state)
	
	if mt_solver.is_running():
		mt_solver.render_intermediate_solutions()
		mt_solver.update()
		spent_time += delta
	elif not finished:
		finished = true
		print('Solved in %.1fs' % spent_time)





"

