script/source = "extends Node2D


var mapper: Mapper2D = TileMapMapper.new()
var rules: WFCRules2D = WFCRules2D.new()
var problem: WFC2DProblem
var solver: WFCSolver


func _ready():
	rules.mapper = mapper
	rules.complete_matrices = true
	rules.learn_from($sample)

	print(rules.axis_matrices[0].format_bits(), rules.axis_matrices[1].format_bits())	

	$sample.visible = false


	@warning_ignore(\"integer_division\")
	problem = WFC2DProblem.new(
		rules,
		$target,
		Rect2i(0,0, 1920/16 + 1, 1080/16 + 1)
		#Rect2i(0,0, 3, 3)
	)
	
	solver = WFCSolver.new(problem)


func _process(_delta):
	if solver.current_state != null:
		for i in range(10):
			if not solver.current_state.is_all_solved():
				solver.solve_step()
		problem.render_state_to_map(solver.current_state)
	else:
		$target.clear()
"

